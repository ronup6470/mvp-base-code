<?xml version="1.0" ?>
<models>
	<model name="Student" group="Student">
		<property name="Id" type="int" jsonProperty="id" gridVisibility="false" formVisibility="false" readOnly="true" labelKey="ID_LABEL"></property>
		<property name="FirstName" jsonProperty="name" type="string" gridVisibility="true" formVisibility="true" readOnly="false" labelKey="NAME_LABEL" placeholderKey="NAME_PLACEHOLDER">
			<validations>
				<validation type="Required" labelKey="NAME.REQUIRED"></validation>
				<validation type="MaxLength" labelKey="NAME.MAXLENGTH">
					<param>15</param>
				</validation>
			</validations>
		</property>
		<property name="School" jsonProperty="school" type="string" gridVisibility="true" formVisibility="true" readOnly="false" labelKey="SCHOOL_LABEL" placeholderKey="SCHOOL_PLACEHOLDER">
			<validations>
				<validation type="Required" labelKey="SCHOOL.REQUIRED"></validation>
			</validations>
		</property>
		<property name="Fees" jsonProperty="fees" type="number" gridVisibility="true" formVisibility="true" readOnly="false" labelKey="FEES_LABEL" placeholderKey="FEES_PLACEHOLDER">
			<validations>
				<validation type="Required" labelKey="FEES.REQUIRED"></validation>
			</validations>
		</property>
		<property name="Subject" jsonProperty="subject" type="string" gridVisibility="true" formVisibility="true" readOnly="false" labelKey="SUBJECT_LABEL" placeholderKey="SUBJECT_PLACEHOLDER">
			<validations>
				<validation type="Required" labelKey="SUBJECT.REQUIRED"></validation>
			</validations>
		</property>
		<property name="BirthDay" jsonProperty="birthday" type="datetime" gridVisibility="true" formVisibility="true" readOnly="false" labelKey="BIRTHDAY_LABEL" placeholderKey="BIRTHDAY_PLACEHOLDER"></property>
		<property name="Photo" jsonProperty="photo" type="file" gridVisibility="true" formVisibility="true" readOnly="false" labelKey="IMAGE_LABEL" placeholderKey="IMAGE_PLACEHOLDER">
			<validations>
				<validation type="Required" labelKey="IMAGE.REQUIRED"></validation>
			</validations>
		</property>
	</model>
</models>